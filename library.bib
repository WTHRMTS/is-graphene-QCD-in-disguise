file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Aguilar, Binosi, Papavassiliou - 2016 - The gluon mass generation mechanism A concise primer.pdf:pdf},
issn = {20950470},
journal = {Frontiers of Physics},
keywords = {Schwinger???Dyson equations,dynamical mass generation,nonperturbative physics},
number = {2},
pages = {1--18},
title = {{The gluon mass generation mechanism: A concise primer}},
volume = {11},
year = {2016}
}
