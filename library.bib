file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Qazilbash et al. - 2007 - Mott transition in VO2 revealed by infrared spectroscopy and nano-imaging.pdf:pdf},
