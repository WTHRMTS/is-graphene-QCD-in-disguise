file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Slezak et al. - 2008 - Imaging the impact on cuprate superconductivity of varying the interatomic distances within individual crystal un.pdf:pdf},
isbn = {0706795105},
issn = {0027-8424},
journal = {Proc. Natl. Acad. Sci. U. S. A.},
number = {9},
pages = {3203--3208},
