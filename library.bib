issn = {1476-4687},
journal = {Nature},
month = {jul},
number = {7304},
pages = {347--51},
pmid = {20631795},
publisher = {Nature Publishing Group},
title = {{Intra-unit-cell electronic nematicity of the high-T(c) copper-oxide pseudogap states.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20631795},
volume = {466},
