isbn = {1098-0121},
issn = {10980121},
journal = {Physical Review B - Condensed Matter and Materials Physics},
number = {12},
pages = {1--11},
primaryClass = {cond-mat},
title = {{Van der Waals density functional: Self-consistent potential and the nature of the van der Waals bond}},
volume = {76},
year = {2007}
}
