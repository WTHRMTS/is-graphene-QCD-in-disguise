author = {Henderson, Thomas M. and Paier, Joachim and Scuseria, Gustavo E.},
doi = {10.1002/pssb.201046303},
file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Henderson, Paier, Scuseria - 2011 - Accurate treatment of solids with the HSE screened hybrid.pdf:pdf},
