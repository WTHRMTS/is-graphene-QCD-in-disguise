@article{Berglund1969a,
abstract = {Optical, thermal, magnetic, and transport properties of VO2<math><mrow><msub><mrow><mi mathvariant="normal">O<mrow><mn>2 in the vicinity of the semiconductor-metal transition temperature of 68°C are discussed in terms of a simple one-electron band picture. Particular emphasis is placed on the variation in these properties from sample to sample. It is shown that all of the available experimental data can be qualitatively explained in terms of such a model. In the metallic state, there are at least two overlapping and partially occupied bands, resulting in a complex Fermi surface characterized by a conductivity effective mass near unity and carrier mobilities in the range between 1 and 10 cm2<math><mrow><msup><mrow><mi mathvariant="normal">cm<mrow><mn>2/V sec. There is no consistent evidence of a very narrow high-mass band near the Fermi level. In the semiconductor state, it is probable that the band overlap described above is removed by the lattice distortion, resulting in an energy gap of approximately 0.6 to 0.7 eV. This energy gap is filled to a density of typically 1019<math><mrow><msup><mrow><mn>10<mrow><mn>19 to 1020<math><mrow><msup><mrow><mn>10<mrow><mn>20 cm−3<math><mrow><msup><mrow><mi mathvariant="normal">cm<mrow><mo>-<mn>3<mn> with donorlike and acceptorlike states, the energy distribution and density depending on the sample, so that it is impossible to obtain an accurate measure of the energy gap. The charge density associated with these donorlike and acceptorlike states is normally larger than that associated with the carriers in the band, resulting in electronic properties characteristic of heavily compensated semiconductors. The conduction-band effective mass and the electron mobility are both near unity in the semiconducting phase of VO2<math><mrow><msub><mrow><mi mathvariant="normal">O<mrow><mn>2. Some implications of these experimental results with respect to the application to VO2<math><mrow><msub><mrow><mi mathvariant="normal">O<mrow><mn>2 of several of the published theories on semiconductor-metal transitions in solids are pointed out.},
author = {Berglund, C. N. and Guggenheim, H. J.},
doi = {10.1103/PhysRev.185.1022},
file = {:C\:/Local_Disk/GWApproximation/Bibliography/PhysRev.185.1022.pdf:pdf},
issn = {0031899X},
journal = {Physical Review},
number = {3},
pages = {1022--1033},
title = {{Electronic Properties of VO2 near the Semiconductor-Metal Transition}},
volume = {185},
year = {1969}
}
@article{Shin1990,
abstract = {Vacuum-ultraviolet reflectance and photoemission spectra of VO2, V2O3, V6O13, and V2O5 are measured in order to investigate the 3d-band structures and electron-correlation effects. In the case of VO2, drastic changes in the 3d (pi* and d||) band structures are found in both spectra through the metal-insulator phase transition. The pi* and d|| bands are found at the Fermi level and Eb=1.3 eV in the photoemission spectra of metallic VO2. In the insulating phase, the pi* valence band in the photoemission spectra becomes empty and a rise of the pi* conduction band by about 0.5 eV is found in the reflectance spectra. This band shift through the phase transition may be a driving force of the metal-insulator transition of VO2. The optical band gap between the d|| valence and pi* conduction bands is obtained as 0.7 eV in the insulating phase, to which the pi*-d|| correlation energy contributes partially. The splitting of the d|| band is also found in the insulating phase. This splitting energy is about 2.5 eV, while the bandwidth of the d|| band is about 1.5 eV. This large band splitting is mainly due to the correlation energy of d|| electrons of about U(d||, d||)=2.1 eV. On the other hand, no drastic change is found in the 3d-band structures of V6O13 and V2O3 except for slight changes in the bandwidths. Furthermore, the density of states at the Fermi level is rather low in these materials even in the metallic phase. These facts support the view that the electron-correlation effects are important and Mott-type metal-insulator transitions are induced in V6O13 and V2O3. Resonant photoemission from V3d and O2p bands are observed as the photon energy is swept through the 3p->3d optical-absorption transition. The resonance profiles of V3d bands show the characteristic antiresonance dip, while those of O2p bands show rather broad and simple enhancements. The multiplet structures in the 3p core photoemission spectra of VO2 and V203 are quantitatively analyzed with use of results from the calculations of Yamaguchi et al.},
