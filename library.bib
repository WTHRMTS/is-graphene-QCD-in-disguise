doi = {10.1017/CBO9780511564079.004},
file = {:C\:/Local_Disk/GWApproximation/Bibliography/Tong QFT/three.pdf:pdf},
isbn = {9780511564079},
journal = {Lectures on Quantum Field Theory},
pages = {32--67},
title = {{3. Interacting Fields}},
