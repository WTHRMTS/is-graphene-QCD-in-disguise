journal = {Phys. Rev. B},
number = {5},
pages = {1801--1815},
title = {{Dimerization of a linear Heisenberg chain in the insulating phases of V$_{1-x}$Cr$_{x}$O$_{2}$}},
volume = {10},
year = {1974}
}
