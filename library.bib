journal = {J. Phys. Chem. C},
keywords = {coatings,doped vanadium dioxide,electronic-structure,phase-transition,sol-gel method,spectra,temperature,thin-films,tungsten,vo2},
number = {5},
pages = {3728--3736},
title = {{Elucidating the Influence of Local Structure Perturbations on the Metal-Insulator Transitions of V$_{1-x}$Mo$_{x}$O$_{2}$ Nanowires: Mechanistic Insights from an X-ray Absorption Spectroscopy Study}},
url = {%3CGo to},
