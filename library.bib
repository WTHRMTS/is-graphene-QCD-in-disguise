file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Novoselov et al. - 2005 - Two-dimensional gas of massless Dirac fermions in graphene.pdf:pdf},
isbn = {9780080431529},
issn = {0028-0836},
journal = {Nature},
number = {7065},
pages = {197--200},
pmid = {16281030},
primaryClass = {cond-mat},
title = {{Two-dimensional gas of massless Dirac fermions in graphene}},
url = {http://www.nature.com/doifinder/10.1038/nature04233},
volume = {438},
year = {2005}
}
