file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bokdam et al. - 2016 - Role of Polar Phonons in the Photo Excited State of Metal Halide Perovskites.pdf:pdf},
issn = {20452322},
journal = {Scientific Reports},
