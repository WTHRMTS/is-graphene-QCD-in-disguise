isbn = {0002-7863},
issn = {1520-5126},
journal = {J. Am. Ceram. Soc.},
number = {12},
pages = {4850--4855},
pmid = {23465080},
title = {{Axially engineered metal-insulator phase transition by graded doping VO2 nanowires}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23465080},
volume = {135},
