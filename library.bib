pmid = {9947970},
title = {{Chiral spin states and superconductivity}},
volume = {39},
year = {1989}
}
@article{Gervais1983,
abstract = {Phonon dispersion curves in rutile TiO2 have been calculated with a rigid-ion model and a shell model. The shell model closely reproduces experimental data. The hardening of soft ferroelectric $\Gamma$1- and $\Gamma$5+ TO modes with increasing temperature, which is observed experimentally, is found to be due to a small simultaneous decrease in effective ionic charge and oxygen polarizability. The decrease in both quantities can be understood in terms of fourth-order inter-ion and oxygen core-shell anharmonic interactions.},
author = {Gervais, F. and Kress, W.},
doi = {10.1103/PhysRevB.28.2962},
file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gervais, Kress - 1983 - Lattice dynamics of incipient ferroelectric rutile TiO2.pdf:pdf},
