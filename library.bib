file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shekhter et al. - 2013 - Bounding the pseudogap with a line of phase transitions in YBa2Cu3O6$\delta$.pdf:pdf},
issn = {1476-4687},
journal = {Nature},
number = {7452},
pages = {75--77},
pmid = {23739425},
