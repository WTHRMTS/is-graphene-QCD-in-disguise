isbn = {0034-6861},
issn = {00346861},
journal = {Rev. Mod. Phys.},
number = {3},
pages = {663--680},
primaryClass = {hep-ph},
title = {{Historical roots of gauge invariance}},
volume = {73},
year = {2001}
}
