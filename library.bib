isbn = {1539-0756},
issn = {00346861},
journal = {Reviews of Modern Physics},
number = {4},
pages = {659--713},
primaryClass = {astro-ph},
title = {{An introduction to lattice gauge theory and spin systems}},
volume = {51},
year = {1979}
}
