file = {:C\:/Local_Disk/GWApproximation/Bibliography/1806.09488.pdf:pdf},
pages = {1--13},
title = {{Spin relaxation due to the D'yakonov-Perel' mechanism in 2D semiconductors with an elliptic band structure}},
url = {http://arxiv.org/abs/1806.09488},
year = {2018}
}
