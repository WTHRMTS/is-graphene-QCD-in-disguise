file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kozik, Ferrero, Georges - 2014 - Non-existence of the Luttinger-Ward functional and misleading convergence of skeleton diagrammatic seri.pdf:pdf},
issn = {0031-9007},
journal = {arXiv Prepr. arXiv1407.5687},
number = {April},
pages = {5},
title = {{Non-existence of the Luttinger-Ward functional and misleading convergence of skeleton diagrammatic series for Hubbard-like models}},
