file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Held et al. - 2011 - Hedin Equations, GW, GWDMFT, and All That.pdf:pdf},
title = {{Hedin Equations, GW, GW+DMFT, and All That}},
url = {http://arxiv.org/abs/1109.3972},
year = {2011}
}
