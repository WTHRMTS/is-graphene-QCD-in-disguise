file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/de la Cruz et al. - 2008 - Magnetic order close to superconductivity in the iron-based layered LaO1-xFxFeAs systems.pdf:pdf},
