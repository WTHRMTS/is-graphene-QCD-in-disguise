issn = {19487185},
journal = {Journal of Physical Chemistry Letters},
number = {13},
pages = {2999--3007},
title = {{Parity-Forbidden Transitions and Their Impact on the Optical Absorption Properties of Lead-Free Metal Halide Perovskites and Double Perovskites}},
volume = {8},
year = {2017}
}
