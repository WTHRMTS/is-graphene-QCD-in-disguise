volume = {9783319017},
year = {2014}
}
@article{Greif2015a,
abstract = {The complexity of quantum many-body systems originates from the interplay of strong interactions, quantum statistics, and the large number of quantum-mechanical degrees of freedom. Probing these systems on a microscopic level with single-site resolution offers unique insight with unprecedented control. Here we report site-resolved imaging of two-component fermionic Mott insulators, metals, and band insulators using ultracold atoms in a square lattice. We observe large, defect-free 2D Mott insulators for strong repulsive interactions. For intermediate interactions we observe a coexistence of phases. From comparison to theory we find trap-averaged entropies per particle of 1.0\,k_{\mathrm{B}} and local entropies as low as 0.5\,k_{\mathrm{B}}. This experiment is a vital step towards probing quantum-mechanical models in regimes inaccessible by modern theoretical methods.},
