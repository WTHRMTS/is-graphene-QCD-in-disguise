issn = {0295-5075},
journal = {Europhys. Lett.},
pages = {851--856},
primaryClass = {arXiv:cond-mat},
title = {{The metal-insulator transition of NbO 2 : An embedded Peierls instability}},
url = {http://stacks.iop.org/0295-5075/58/i=6/a=851?key=crossref.96919966df73a11b8b9ba743f7c8652b},
