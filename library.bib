file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ziesche et al. - 2000 - How Correlation Suppresses Density Fluctuations in the Uniform Electron Gas of One , Two , or Three Dimensions.pdf:pdf},
isbn = {1097461X},
issn = {0020-7608 (print), 1097-461X (electronic)},
journal = {Int. J. Quantum Chem.},
keywords = {correlation,fluctuation,reduced dimensionality,uniform electron gas},
number = {2000},
pages = {819--830},
title = {{How Correlation Suppresses Density Fluctuations in the Uniform Electron Gas of One , Two , or Three Dimensions}},
volume = {77},
year = {2000}
}
