@article{Kaminski2002,
abstract = {A change in 'symmetry' is often observed when matter undergoes a phase transition-the symmetry is said to be spontaneously broken. The transition made by underdoped high-transition-temperature (high-Tc) superconductors is unusual, in that it is not a mean-field transition as seen in other superconductors. Rather, there is a region in the phase diagram above the superconducting transition temperature Tc (where phase coherence and superconductivity begin) but below a characteristic temperature T* where a 'pseudogap' appears in the spectrum of electronic excitations. It is therefore important to establish if T* is just a cross-over temperature arising from fluctuations in the order parameter that will establish superconductivity at Tc (refs 3, 4), or if it marks a phase transition where symmetry is spontaneously broken. Here we report that, for a material in the pseudogap state, left-circularly polarized photons give a different photocurrent from right-circularly polarized photons. This shows that time-reversal symmetry is spontaneously broken below T*, which therefore corresponds to a phase transition.},
archivePrefix = {arXiv},
arxivId = {cond-mat/0306140},
author = {Kaminski, a and Rosenkranz, S and Fretwell, H M and Campuzano, J C and Li, Z and Raffy, H and Cullen, W G and You, H and Olson, C G and Varma, C M and H{\"{o}}chst, H},
doi = {10.1038/416610a},
eprint = {0306140},
file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kaminski et al. - 2002 - Spontaneous breaking of time-reversal symmetry in the pseudogap state of a high-Tc superconductor.pdf:pdf},
issn = {00280836},
journal = {Nature},
number = {6881},
pages = {610--613},
pmid = {11948344},
primaryClass = {cond-mat},
title = {{Spontaneous breaking of time-reversal symmetry in the pseudogap state of a high-Tc superconductor.}},
volume = {416},
year = {2002}
}
@article{Kohsaka2012,
abstract = {High temperature superconductivity emerges from the antiferromagnetic Mott state when holes are introduced into the CuO2 plane. The electronic structure created by that process1 is not understood, although changes in the electronic state of oxygen atoms appear paramount2-5. Hole doping first destroys the Mott state yielding a weak insulator6,7 where electrons localize only at low temperatures without a full energy gap. At higher doping, the pseudogap state, a weakly conducting state with a large anisotropic energy gap and intra-unit-cell breaking of 90{\textordmasculine}-rotational (C4v) symmetry appears3,4,8-10. However, a direct visualization of the emergence of these phenomena with increasing hole density has never been achieved. Here we report atomic-scale imaging of electronic structure evolution from the weak-insulator through the emergence of the pseudogap to the superconducting state in Ca2-xNaxCuO2Cl2. We find that the spectral signature characteristic of the pseudogap emerges at lowest doping from a weakly insulating but C4v-symmetric matrix that has its own characteristic spectral shape. Then at slightly higher hole-density, nanoscale regions exhibiting pseudogap spectra and 180{\textordmasculine}-rotational (C2v) symmetry form unidirectional clusters within the 2 C4v-symmetric matrix. These findings indicate that doping proceeds by the appearance of nanoscale clusters of localized holes within which the broken-symmetry pseudogap state is stabilized. A fundamentally two-component electronic structure11 then exists until the C2v-symmetric clusters touch each other at higher doping. It is at this stage that the long-range high-Tc superconductivity appears in Ca2-xNaxCuO2Cl2},
author = {Kohsaka, Y and Hanaguri, T and Azuma, M and Takano, M and Davis, J C and Takagi, H},
doi = {10.1038/nphys2321},
file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kohsaka et al. - 2012 - Visualization of the emergence of the pseudogap state and the evolution to superconductivity in a lightly hole-d.pdf:pdf},
isbn = {1745-2473},
issn = {1745-2473},
journal = {Nat. Phys.},
number = {7},
pages = {534--538},
publisher = {Nature Publishing Group},
title = {{Visualization of the emergence of the pseudogap state and the evolution to superconductivity in a lightly hole-doped Mott insulator}},
url = {http://dx.doi.org/10.1038/nphys2321},
volume = {8},
year = {2012}
}
