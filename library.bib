eprint = {1310.3744},
file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nam et al. - 2013 - Superconducting fluctuations in organic molecular metals enhanced by Mott criticality.pdf:pdf},
issn = {2045-2322},
journal = {Sci. Rep.},
pages = {3390},
pmid = {24292063},
title = {{Superconducting fluctuations in organic molecular metals enhanced by Mott criticality.}},
