file = {:C\:/Users/e09250/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kasahara et al. - 2012 - Electronic nematicity above the structural and superconducting transition in BaFe2(As1−xPx)2.pdf:pdf},
isbn = {0028-0836},
issn = {0028-0836},
journal = {Nature},
number = {7403},
pages = {382--385},
