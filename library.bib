issn = {01631829},
journal = {Phys. Rev. B},
number = {8},
pages = {4993--5009},
pmid = {9994356},
title = {{Vacuum-ultraviolet reflectance and photoemission study of the metal-insulator phase transitions in VO2, V6O13, and V2O3}},
volume = {41},
year = {1990}
